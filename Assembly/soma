.data

	Numero_um: .asciiz "Informe o primeiro número: "
	Numero_dois: .asciiz "Informe o segundo numero: "
	Soma: .asciiz "A soma é: "

.text

	li $v0, 4
	la $a0,Numero_um
	syscall
	
	li $v0,5
	syscall
	
	move $t0,$v0
	
	li $v0,4
	la $a0,Numero_dois
	syscall
	
	li $v0,5
	syscall
	
	move $t1,$v0
	
	add $t2, $t0, $t1
	
	li $v0,4
	la $a0,Soma
	syscall
	
	li $v0,1
	move $a0,$t2
	syscall